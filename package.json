{
  "name": "easyfetch-client",
  "displayName": "EasyFetch Client",
  "publisher": "Mhocine",
  "description": "EasyFetch Client is a vs code extension that will help you test your apis directly from vs code. you can activate it by ctrl+shift+p then type Easy fetch Client",
  "version": "0.0.3",
  "engines": {
    "vscode": "^1.95.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hocineMK03/EasyFetch-Client.git"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "easyfetch-client.EasyFetch",
        "title": "Easy Fetch Client"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.9",
    "@types/node": "20.x",
    "@types/vscode": "^1.95.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.13.0"
  },
  "dependencies": {
    "@hocinemk/easyfetch": "^1.0.4"
  }
}
